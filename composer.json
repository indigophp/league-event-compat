{
    "name": "indigophp/event-compat",
    "description": "Event compatibility layer for Symfony EventDispatcher",
    "license": "MIT",
    "keywords": ["league", "symfony", "event"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/event-dispatcher": "~2.6.0",
        "league/event": "~2.0.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Event\\Compat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Event\\Compat\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
